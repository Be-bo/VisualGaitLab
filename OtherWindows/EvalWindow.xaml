<Window x:Class="VisualGaitLab.OtherWindows.EvalWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisualGaitLab.OtherWindows"
        mc:Ignorable="d"
        Title="Evaluation" Height="230" Width="300"
        WindowStartupLocation="CenterScreen"
        Icon="/Images/logo.png">
    <StackPanel>
        <Label Content="Training Time" Margin="5, 5, 5, 0"/>
        <TextBlock x:Name="TimeTextBlock" Style="{StaticResource GaitTextBox}" Margin="5, 0, 5, 0"/>
        <Label Content="Train error(px)" Margin="5, 5, 5, 0"/>
        <TextBlock x:Name="TrainErrorTextBlock" Style="{StaticResource GaitTextBox}" Margin="5, 0, 5, 0"/>
        <Label Content="Test error(px)" Margin="5, 5, 5, 0"/>
        <TextBlock x:Name="TestErrorTextBlock" Style="{StaticResource GaitTextBox}" Margin="5, 0, 5, 0"/>
        <Label Content="p-cutoff" Margin="5, 5, 5, 0"/>
        <TextBlock x:Name="PCutoffTextBlock" Style="{StaticResource GaitTextBox}" Margin="5, 0, 5, 0"/>
    </StackPanel>
</Window>
